{
	"user": "rsetiawan7",
	"repo": "MapAndreasDebugger",
	"entry": "MapAndreasDebugger.pwn",
	"output": "../MapAndreasDebugger.amx",
	"dependencies": [
		"sampctl/samp-stdlib",
		"rsetiawan7/samp-plugin-mapandreas:v1.2.3"
	],
	"builds": [
		{
			"name": "dev",
			"version": "3.10.7",
			"args": [
				"-;+",
				"-(+",
				"-d3",
				"-Z+"
			],
			"includes": [
				"../../pawno/include"
			],
			"constants": {
				"MAX_PLAYERS": "10"
			},
			"compiler": {}
		},
		{
			"name": "live:debug",
			"version": "3.10.7",
			"args": [
				"-;+",
				"-(+",
				"-d3",
				"-Z+"
			],
			"includes": [
				"../../pawno/include"
			],
			"constants": {
				"MAX_PLAYERS": "300"
			},
			"compiler": {}
		},
		{
			"name": "live",
			"version": "3.10.7",
			"args": [
				"-;+",
				"-(+",
				"-O2",
				"-Z+"
			],
			"includes": [
				"../../pawno/include"
			],
			"constants": {
				"MAX_PLAYERS": "300"
			},
			"compiler": {}
		}
	]
}